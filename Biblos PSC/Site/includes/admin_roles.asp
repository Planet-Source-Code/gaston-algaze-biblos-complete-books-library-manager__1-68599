Sub GetTablesPrivileges(sXML)
Dim oRs
Dim oDOM

	Set oDOM = Server.CreateObject("MSXML2.DOMDocument")
	Set oRs = Server.CreateObject("ADODB.Recordset")
	
	If oDOM.loadXML(sXML) Then
		Set oRs = RecordsetFromXMLDocument(oDOM)
		
		While Not oRs.EOF
			if Instr(1,  oRs("Tabla"), "_", 1) or oRs("Tabla") = "tablas" or oRs("Tabla") = "campos" or oRs("Tabla") = "restricciones" or oRs("Tabla") = "operaciones" or oRs("Tabla") = "roles" then 
				oRs.MoveNext
			else
				DrawTablesPrivileges oRs("Tabla"), IIf(isNull(oRs("Permiso")), "0", oRs("Permiso"))
				oRs.MoveNext
			End if
		Wend
	End if

End Sub

