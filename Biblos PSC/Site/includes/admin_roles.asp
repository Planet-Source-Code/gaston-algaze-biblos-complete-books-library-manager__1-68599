Sub DrawRestrictions(sXML, iRolID)
Dim bDeleted
Dim oRs
Dim oDOM
Dim j

	Set oDOM = Server.CreateObject("MSXML2.DOMDocument")
	Set oRs = Server.CreateObject("ADODB.Recordset")
	
	If oDOM.loadXML(sXML) Then
		'oRs.CursorLocation = 3
		Set oRs = RecordsetFromXMLDocument(oDOM)
		
		'oRS.Sort = "ID ASC"

		While Not oRs.EOF
			
			bDeleted = IIf(Len(oRs("Fecha_Baja")) > 0, true, false)
			If bDeleted then
				response.write "<tr bgcolor=""#F2AAB1"">"
			Else
				response.write "<tr>"
			End If		
			response.write "	<td class=""h_text""><div align=""center"">" & oRs("tabla") & "</div></td>"
			response.write "	<td class=""h_text""><div align=""center"">" & oRs("campo") & "</td>"
			response.write "	<td><div align=""center""><span class=""h_text"">" & oRs("operacion") & "</span></div></td>"
			response.write "	<td><div align=""center""><span class=""h_text"">""" & oRs("valor") & """</span></div></td>"
			response.write "<td class=""l_text"" bgcolor=""#FFFFFF""><div align=""center""><a class=""l_text"" onClick=""myRef = window.open('restricciones_update.asp?ID=" & oRs("ID") & "&IDBack=" & iRolID & "','mywin','left=200,top=200,width=600,height=1,scrollbars=0,toolbar=0,resizable=0,menubar=0'); myRef.focus()"" href=""javascript:void(0);"">editar</a></div></td>"		
			response.write "<td class=""l_text"" bgcolor=""#FFFFFF""><div align=""center"">"

		If bDeleted then
			response.write "<a class=""l_text"" href=""restricciones_recover.asp?ID=" & oRs("ID") & "&IDback=" & iRolID & """ onclick=""return confirm('Est seguro que desea recuperar el registro?')"">recuperar</a>"
		Else
			response.write "<a class=""l_text"" href=""restricciones_delete.asp?ID=" & oRs("ID") & "&IDback=" & iRolID & """ onclick=""return confirm('Est seguro que desea eliminar el registro?')"">eliminar</a>"
		End If

		response.write "</div></td>"
		response.write "</tr>"

			oRs.MoveNext
		Wend

	End If

End Sub

Function Bin2Dec(Num)
Dim n, a, x
     n = Len(Num) - 1      
	 a = n 
	 Bin2Dec = 0
     Do While n > -1 
        x = Mid(Num, ((a + 1) - n), 1) 
        Bin2Dec = IIf((x = "1"), Bin2Dec + (2 ^ (n)), Bin2Dec) 
        n = n - 1 
     Loop 
End Function
